{
  "name": "vue-i18n",
  "description": "Internationalization plugin for Vue.js",
  "version": "2.2.0",
  "author": {
    "name": "kazuya kawaguchi",
    "email": "kawakazu80@gmail.com"
  },
  "bugs": {
    "url": "https://github.com/kazupon/vue-i18n/issues"
  },
  "devDependencies": {
    "babel": "^5.8.23",
    "babel-core": "^5.8.23",
    "babel-loader": "^5.3.2",
    "babel-plugin-espower": "^1.0.0",
    "eslint": "^1.3.1",
    "espower-babel": "^3.3.0",
    "istanbul-instrumenter-loader": "^0.1.3",
    "json-loader": "^0.5.2",
    "karma": "^0.13.9",
    "karma-coverage": "^0.2.6",
    "karma-coveralls": "^0.1.4",
    "karma-mocha": "^0.2.0",
    "karma-mocha-reporter": "^1.1.1",
    "karma-phantomjs-launcher": "^0.2.1",
    "karma-sourcemap-loader": "^0.3.5",
    "karma-webpack": "^1.7.0",
    "mocha": "^2.3.1",
    "nightmare": "^1.6.5",
    "phantomjs": "^1.9.18",
    "power-assert": "^1.0.0",
    "vue": "^0.12.0",
    "webpack": "^1.12.1"
  },
  "homepage": "https://github.com/kazupon/vue-i18n",
  "keywords": [
    "i18n",
    "internationalization",
    "plugin",
    "vue",
    "vue.js"
  ],
  "license": "MIT",
  "main": "dist/vue-i18n.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/kazupon/vue-i18n.git"
  },
  "scripts": {
    "build": "babel src --out-dir lib --source-maps-inline",
    "bundle": "webpack --config config/webpack.conf.js && webpack --config config/webpack.min.conf.js",
    "clean": "rm -rf lib && rm -rf coverage && rm -rf dist",
    "e2e": "mocha --timeout 200000 --compilers js:espower-babel/guess test/e2e/translation.js",
    "lint": "eslint src test config",
    "test": "karma start config/karma.conf.js",
    "watch": "babel src --out-dir lib --watch --source-maps-inline"
  }
}
